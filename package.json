{
  "name": "todo5",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "deploy": "pushd ../nstielau.github.com; git pull origin master && git commit -am 'Updating todo5' &&  git push origin master; popd",
    "build": "vite build --base /todo5/ --outDir ../nstielau.github.com/todo5",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@doist/todoist-api-typescript": "^2.1.2",
    "@quasar/extras": "^1.15.11",
    "axios": "^1.3.4",
    "dompurify": "^3.0.1",
    "hammerjs": "^2.0.8",
    "marked": "^4.2.12",
    "quasar": "^2.11.7",
    "uuid": "^9.0.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.3.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "sass": "^1.32.12",
    "vite": "^4.1.4"
  }
}
